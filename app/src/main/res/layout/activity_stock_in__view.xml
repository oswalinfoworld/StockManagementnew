<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.oswal.stockmangmentnew.Services.StockIn.Stock_IN_View">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Stock IN View "
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="25dp"
            android:background="#F77F76"
            />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="List of Supplier name :"
        android:layout_marginTop="10dp"/>

              <ListView
                  android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                  android:divider="#f00"
                  android:dividerHeight="1dp"
                  android:layout_marginTop="0dp"
                  android:id="@+id/stockIn_listview"
                  android:listSelector="#2196F3"></ListView>

</LinearLayout>